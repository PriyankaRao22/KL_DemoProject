<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaFX.CustomerCartInfo">
   <children>
      <TableView fx:id="cartTableView" layoutX="4.0" layoutY="129.0" prefHeight="354.0" prefWidth="587.0">
        <columns>
          <TableColumn fx:id="Carttype" prefWidth="71.0" text="Type" />
            <TableColumn fx:id="Cartbreed" prefWidth="132.0" text="Breed" />
            <TableColumn fx:id="Cartage" prefWidth="128.0" text="Age" />
            <TableColumn fx:id="Cartprice" prefWidth="94.0" text="Price" />
            <TableColumn fx:id="Cartavailability" prefWidth="161.0" text="Availability" />
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="38.0" mnemonicParsing="false" onAction="#cartRemoveButtonOnAction" prefHeight="25.0" prefWidth="74.0" style="-fx-background-color: Orange;" text="Remove" />
      <Button layoutX="143.0" layoutY="38.0" mnemonicParsing="false" onAction="#cartClearButtonOnAction" prefHeight="25.0" prefWidth="74.0" style="-fx-background-color: Orange;" text="Clear" />
      <Label fx:id="CartMessageDisplay" layoutX="72.0" layoutY="71.0" prefHeight="25.0" prefWidth="278.0" />
      <Button layoutX="243.0" layoutY="38.0" mnemonicParsing="false" onAction="#cartDetails" prefHeight="25.0" prefWidth="172.0" style="-fx-background-color: Orange;" text="CartDetails" />
      <Button layoutX="14.0" layoutY="2.0" mnemonicParsing="false" onAction="#cartBackOnAction" style="-fx-background-color: Orange;" text="Back" />
      <Button layoutX="477.0" layoutY="15.0" mnemonicParsing="false" onAction="#payButtonOnAction" prefHeight="25.0" prefWidth="106.0" style="-fx-background-color: orange;" text="Pay" />
   </children>
</AnchorPane>
